AIC<-read.table("Lhoods_oct_2021.txt", sep = "\t", header = T)
View(AIC)
aics<-AIC[order(-AIC$AIC),]
View(aics)
for(i in 1:dim(aics)[1]){
aics$deltaAIC[i]<-aics$AIC[i]-aics$AIC[10]}
aics$wi<-exp(-0.5*aics$deltaAIC)
aics$aic.weights<-aics$wi/sum(aics$wi)
write.table(aics, "Lhoods_aicW_oct2021.txt", sep="\t", quote = F)
